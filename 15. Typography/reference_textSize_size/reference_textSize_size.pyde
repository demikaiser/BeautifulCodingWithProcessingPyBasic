def setup():
    size(640, 360)
    myFont = createFont("Roboto Bold", 48)
    textFont(myFont)
    
def draw():
    background(255)
    textSize(64)
    fill(200, 0, 0, 255)
    text("Roboto Bold Font", 30, 60)
    textSize(48)
    fill(200, 0, 0, 128)
    text("Roboto Bold Font", 30, 120)
    textSize(24)
    fill(200, 0, 0, 32)
    text("Roboto Bold Font", 30, 180)
    
    
    

    
